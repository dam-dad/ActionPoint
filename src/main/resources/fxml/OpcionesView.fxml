<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="view" alignment="TOP_CENTER" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	spacing="5.0" xmlns="http://javafx.com/javafx/17"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ImageView fx:id="avatarImages" pickOnBounds="true"
			preserveRatio="true">
			<image>
				<Image url="@../images/64.jpg" />
			</image>
			<viewport>
				<Rectangle2D />
			</viewport>
		</ImageView>
		<JFXToggleButton fx:id="temaToggle" text="Tema" />
		<ComboBox fx:id="filtrosCombo"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			promptText="                    Fltros" VBox.vgrow="ALWAYS" />
		<Button fx:id="historialButton"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" onAction="#onHistorialAction"
			text="Historial" VBox.vgrow="ALWAYS" />
		<Button fx:id="avatarButton" maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" mnemonicParsing="false"
			onAction="#onCambiarAction" text="Cambiar Avatar" VBox.vgrow="ALWAYS" />
		<Button fx:id="cerrarCesionButton"
			maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			mnemonicParsing="false" onAction="#onCerrarAction"
			text="Cerrar cesion" VBox.vgrow="ALWAYS" />
	</children>
</VBox>
